normalOrder = fn(a,b){
	sort([a,b])
}

reverseOrder = fn(a,b){
	reverse(sort([a,b]))
}

log(normalOrder(20,10))

//10, 20

log(reverseOrder(300,500))

//500, 300

xs = sortBy([1,2,3,4,5], reverseOrder)
log(xs)
//xs == 5,4,3,2,1

xs = sortBy([5, 50, 10, 100, 6, 60, 7, 70], normalOrder)
log(xs)

//xs == 5, 6, 7, 10, 50, 60, 70, 100

xs = sortBy([1,2,3,4,5], fn(a,b){
	reverse(sort([a,b]))
})
log(xs)
//xs == 5,4,3,2,1


