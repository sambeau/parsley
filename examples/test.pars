Table = fn({rows}){
	<table>{
	for(cols in rows){
		<tr>{
		for (col in cols){
			<td>{col}</td>
		}}
		</tr>
	}}
	</table>
}

Page = fn({contents}){
	<html>
		<head>
		</head>
		<body>
			{contents}
		</body>
	</html>
}

main = fn(){
	<Page>
		<Table rows={[
			[1,2,3],
			[4,5,6],
			[7,8,9]
			]}
		/>
	</Page>
}

main()