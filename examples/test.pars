xs = "sam","_poo","was","_poo","here!","_poo"

for (x in xs) {
	y = toUpper(x)
	if (x[0] != "_")
		return y
}

head = fn(xs){
	y,_ = xs
	y
}

tail = fn(xs){
	_,ys = xs
	ys
}

xs = 1,2,3,4,5
h = head(xs)
ts = tail(xs)
_ = tail(xs)
print(h,"|",_,ts,_)